{% form_theme form 'GNKWLDFLdfcorpBundle:Form:bootstrap.html.twig' %}
<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register form-register">
    <h2 class="form-register-heading">{{ 'layout.register'|trans({}, 'FOSUserBundle') }}</h2>
    {{ form_widget(form) }}
    <div>
        <input class="btn btn-lg btn-primary btn-block" type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" />
    </div>
</form>
